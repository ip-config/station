{
  "manifest_version": 2,
  "name": "Station CX",
  "version": "1.0.0",

  "options_page": "index.html",
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "icon-32.png"
  },

  "icons": {
    "128": "icon-128.png"
  },

  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
